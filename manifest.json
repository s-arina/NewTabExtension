{
    "short_name": "New Tab",
    "name": "New Tab Extension",
    "description": "A cute new tab page.",
    "version": "1.0.0",
    "manifest_version": 3,
    "permissions": [
      "storage",
      "tabs",
      "activeTab",
      "geolocation"
    ],
    "icons": {
      "16": "/images/favicon.png",
      "32": "/images/favicon.png",
      "48": "/images/favicon.png",
      "128": "/images/favicon.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "chrome_url_overrides": {
        "newtab": "extension/build/index.html"
    }
}